
@font-face {
font-family: "{{ fontName }}";
src: {{{ src }}};
}

{{# if baseSelector }}
    {{ baseSelector }}:before {
{{ else }}
    {{ baseTag }}[class^="{{classPrefix}}"]:before, {{ baseTag }}[class*=" {{classPrefix}}"]:before {
{{/ if }}
font-family: {{ fontName }} !important;
font-style: normal;
font-weight: normal !important;
font-variant: normal;
text-transform: none;
font-size: 16px;
line-height: 1;
color: #5c7079;
-webkit-font-smoothing: antialiased;
-moz-osx-font-smoothing: grayscale;
}

{{# each codepoints }}
    {{# if ../baseSelector }}
        {{ ../baseSelector }}.{{ ../classPrefix }}{{ @key }}:before {
    {{ else }}
        {{ baseTag }}.{{ ../classPrefix }}{{ @key }}:before {
    {{/ if }}
    content: "\\{{ this }}";
    }
{{/ each }}

.overlay-icon:before {
    font-size: 8px!important;
    position: absolute;
    margin-left: -16px;
}

.overlay-icon-tl:before {
    font-size: 8px!important;
    position: absolute;
    margin-left: -16px;
}

.overlay-icon-tr:before {
    font-size: 8px!important;
    position: absolute;
    margin-left: -8px;
}

.overlay-icon-bl:before {
    font-size: 8px!important;
    position: absolute;
    margin-top: 8px;
    margin-left: -16px;
}

.overlay-icon-br:before {
    font-size: 8px!important;
    position: absolute;
    margin-top: 8px;
    margin-left: -8px;
}

.warning:before {
    color: orange!important;
}

.success:before {
    color: green!important;
}

.error:before {
    color: red!important;
}

.primary:before {
    color: #5c7079!important;
}